<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.springframework.session.jdbc" level="debug"/>

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>